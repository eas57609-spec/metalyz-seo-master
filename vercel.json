{
  "framework": "nextjs",
  "regions": ["sfo1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-SSL-Renewal",
          "value": "forced"
        }
      ]
    }
  ],
  "functions": {
    "src/app/api/webhooks/payment/route.ts": {
      "maxDuration": 30
    }
  }
}